{
    "extends": [
        "airbnb"
    ],
    "parserOptions": {
        "ecmaVersion": 2018,
        "sourceType": "module",
        "ecmaFeatures": {
            "jsx": true
        }
    },
    "env": {
        "es6": true,
        "browser": true,
        "node": true
    },
    "rules": {
        "linebreak-style": ["error", "unix"],
        "react/prefer-stateless-function": 0,
        "import/no-unresolved": false
    }
}